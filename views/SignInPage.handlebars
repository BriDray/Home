<body>
    <!-- THIS IS THE HEADER OF THE PAGE -->
    <header class="header"></header>
    <div Class="signInPage">
        <h1>WELCOME</h1>
        <h2><img class="homeLogo-SignIn" src="/images/HOME.png" width="625px" height="179px"></h2>
        <!-- SIGN IN FORM TAKEN FROM MATERIALIZE -->
        <div class="row">
         <form class="col s12 form" id="signIn">
            <div class="input-field col s12">
                <input id="e-mail" type="email" class="validate">
                <label for="email">Email</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="Password" type="password" class="validate">
                <label for="password">Password</label>
            </div>
        </div>
         <button class="waves-effect waves-light btn signInBtn" id="signInButton" type="submit">Sign In</button>
        </form>
    </div>
    <!-- SIGN IN BUTTON -->
   
    <!-- DON'T HAVE AN ACCOUNT? -->
    <div class="signUp">
        <h4>Don't have an account?</h4>
        <a class="signUpA" href="/signUpPage">Sign-up</a>
    </div>
    <!-- Compiled and minified JavaScript FROM MATERIALIZE -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="/js/signIn.js"></script>
</body>

</html>